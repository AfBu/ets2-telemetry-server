<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="info.funbit.ets.telemetry" android-versionCode="1" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>ETS2 Gauge</name>
  <description>Mobile gauge for Euro Truck Simulator 2.</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Funbit</author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.0" />
  <preference name="windows-phone-target-version" value="8.1" />
  <preference name="Orientation" value="landscape" />
  <preference name="Fullscreen" value="true" />
  <preference name="BackupWebStorage" value="cloud" />
  <preference name="KeepRunning" value="False" />
  <preference name="InAppBrowserStorageEnabled" value="True" />
  <vs:plugin name="org.apache.cordova.splashscreen" version="0.3.3" />
  <vs:plugin name="nl.x-services.plugins.insomnia" version="4.0.1" />
  <vs:plugin name="me.apla.cordova.app-preferences" version="0.4.2" />
  <vs:platformSpecificValues />
</widget>